<Window x:Class="Project.Views.FeedbackModal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project.Views"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="FeedbackModal" Height="463" Width="567">
    <Grid Margin="0,0,0,-44">
        <Label Content="Issue type:" Margin="23,10,459,428"/>
        <ComboBox Margin="23,48,391,400" Height="28">
            <ComboBoxItem>Issue 1</ComboBoxItem>
            <ComboBoxItem>Issue 2</ComboBoxItem>
        </ComboBox>
        <TextBox Margin="10,95,10,138" TextWrapping="Wrap"></TextBox>
        <Button Content="Send" Margin="23,351,459,76"/>
        <Button Content="Cancel" Margin="164,351,284,76" Click="Cancel_Click"/>
    </Grid>
</Window>
